FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 10.0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    walls
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    emptyWalls
    {
        type            empty;
    }
    bottom
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    top
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    object
    {
        type            omegaWallFunction;
        value           $internalField;
    }
}

// ************************************************************************* //
