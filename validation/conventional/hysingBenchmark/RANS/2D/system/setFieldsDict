FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
);

regions
(
    cylinderToCell
    {
        p1 (0 0.5 -0.1);
        p2 (0 0.5  0.2); 
        radius  0.25;
        fieldValues
        (
            volScalarFieldValue alpha.water 0
        );
    }
);

