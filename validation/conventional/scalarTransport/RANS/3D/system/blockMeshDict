// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters	1;

cells			10;
ogridCells		1;

hRef			1;
lRef			1;

D				0.008;
ogrid			0.001;

xmin			0;
ymin			0;
zmin			0;

r				#calc "0.5 * $D";
ri				#calc "$r - $ogrid";
riO				#calc "0.925 * $ri";

x1				#calc "0.100 - $r";
x2				#calc "0.200 - $r";

y				#calc "0.054 - $r";

z1				0.150;
z2				0.108;

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

x1Cells			#calc "round($cells * $x1 / $D)";
x2Cells			#calc "round($cells * $x2 / $D)";

yCells			#calc "round($cells * $y / $D)";

z1Cells			#calc "round( 2 * ($cells - 1 + $ogridCells) * $z1 / $D )";
z2Cells			#calc "round($cells * $z2 / $D)";

throatCells		#calc "round($cells - 1)";

