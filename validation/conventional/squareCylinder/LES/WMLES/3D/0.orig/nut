FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    object
    {
        type            LOTWWallModel;
        value           uniform 0;
		RootFinder
		{
			type			Newton;
		}
		Law
		{
			type			IntegratedWernerWengle;
		}
    }
    front
    {
        type            cyclic;
    }
    back
    {
        type            cyclic;
    }
}

